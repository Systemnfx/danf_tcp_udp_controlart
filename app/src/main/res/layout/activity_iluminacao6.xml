<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="com.example.danftcpudp.Iluminacao6">

    <ImageView
        android:id="@+id/imagebar19"
        android:layout_width="221dp"
        android:layout_height="29dp"
        android:layout_marginTop="140dp"
        android:src="@drawable/bg_iluminacao"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/image_17"
        android:layout_width="90dp"
        android:layout_height="29dp"
        android:layout_marginTop="140dp"
        android:src="@drawable/botao_ilumin_off"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.657"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_25"
        android:layout_width="45dp"
        android:layout_height="40dp"
        android:background="#00FFFFFF"
        android:scaleType="centerInside"
        android:src="@drawable/botao_voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044"
        tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/FEED_C201"
        android:layout_width="70dp"
        android:layout_height="46dp"
        android:layout_marginTop="132dp"
        android:scaleType="centerInside"
        android:src="@drawable/ilumin_off"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toEndOf="@+id/image_17"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/OFF_C502"
        android:layout_width="45dp"
        android:layout_height="29dp"
        android:onClick="OFFFC701"
        app:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/image_17"
        app:layout_constraintEnd_toStartOf="@+id/ON_C602"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/image_17"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/ON_C602"
        android:layout_width="45dp"
        android:layout_height="29dp"
        android:onClick="OFONC701"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#00FFFFFF"
        android:textColorHint="#00FAFAFA"
        android:textColorLink="#00FFFFFF"
        android:visibility="visible"
        app:backgroundTint="#00FFFFFF"
        app:iconTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/image_17"
        app:layout_constraintEnd_toEndOf="@+id/image_17"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/image_17"
        app:layout_constraintVertical_bias="0.0"
        app:rippleColor="#00FFFDFD"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/FEED_ON_C701"
        android:layout_width="70dp"
        android:layout_height="46dp"
        android:layout_marginTop="132dp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.625"
        app:layout_constraintStart_toEndOf="@+id/image_17"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icone_iluminacao_on_fundo" />

    <TextView
        android:id="@+id/c801"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="CENTRAL"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/imagebar19"
        app:layout_constraintEnd_toEndOf="@+id/imagebar19"
        app:layout_constraintStart_toStartOf="@+id/imagebar19"
        app:layout_constraintTop_toTopOf="@+id/imagebar19" />

    <TextView
        android:id="@+id/saladeestar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:fontFamily="sans-serif"
        android:text="ESCRITÃ“RIO"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.061" />
</androidx.constraintlayout.widget.ConstraintLayout>